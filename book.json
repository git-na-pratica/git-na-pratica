{
    "language": "pt",
    "structure": {
        "glossary": "glossario.md",
        "summary": "sumario.md"
    },
    "plugins": [
        "codeblock-disable-glossary",
        "anchors",
        "layout",
        "disqus",
        "extend-default-theme",
        "search",
        "exercises",
        "image-captions",
        "gist",
        "mathjax",
        "indexes",
        "customtheme",
        "theme-api",
        "theme-default",
        "comment"
    ],
    "pluginsConfig": {
        "layout": {
            "headerPath": "layouts/header.html",
            "footerPath": "layouts/footer.html"
        },
        "disqus": {
            "shortName": "gitnapratica",
            "useIdentifier": true
        },
        "image-captions": {
            "caption": "Image _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_",
            "list_caption": "List image _BOOK_IMAGE_NUMBER_: _CAPTION_",
            "variable_name": "picturesList"
        }
    },
    "variables": {
        "Version": "0.1"
    }
}